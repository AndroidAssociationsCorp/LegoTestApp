<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.legotestapp"
    android:versionName="1.0"
    android:versionCode="1">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ico"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ico"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.legotestapp.Level1"
            android:screenOrientation="fullSensor"/>
        <activity android:name="com.example.legotestapp.MainActivity"
            android:screenOrientation="fullSensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.legotestapp.SecondActivity"
            android:screenOrientation="fullSensor">
        </activity>
        
        <activity
            android:name="com.example.legotestapp.NewbieActivity"
            android:screenOrientation="fullSensor">
        </activity>
        <activity
            android:name=".NewbieResultActivity"
            android:screenOrientation="fullSensor">
        </activity>

        <activity
            android:name="com.example.legotestapp.ProffActivity"
            android:screenOrientation="fullSensor">
        </activity>
        <activity
            android:name="com.example.legotestapp.ProffActivity2"
            android:screenOrientation="fullSensor">
        </activity>
        <activity
            android:name="com.example.legotestapp.ProffActivity3"
            android:screenOrientation="fullSensor">
        </activity>
    </application>

</manifest>